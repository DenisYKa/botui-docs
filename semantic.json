{
  "base": "semantic/",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "dist/",
      "uncompressed": "dist/components/",
      "compressed": "dist/components/",
      "themes": "dist/themes/"
    },
    "clean": "dist/"
  },
  "permission": false,
  "autoInstall": false,
  "rtl": false,
  "components": [
    "reset",
    "site",
    "button",
    "container",
    "divider",
    "header",
    "icon",
    "image",
    "label",
    "list",
    "rail",
    "segment",
    "breadcrumb",
    "grid",
    "menu",
    "message",
    "table",
    "accordion",
    "dropdown",
    "sidebar",
    "sticky",
    "tab",
    "transition",
    "api",
    "state",
    "visibility"
  ],
  "version": "2.2.10"
}